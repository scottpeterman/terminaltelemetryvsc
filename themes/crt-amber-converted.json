{
  "name": "Imported Custom Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#FFB000",
    "activityBar.background": "#000000",
    "sideBar.background": "#000000",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFB000",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#CC8800",
    "terminal.ansiGreen": "#FFAA00",
    "terminal.ansiYellow": "#FFB000",
    "terminal.ansiBlue": "#CC8800",
    "terminal.ansiMagenta": "#CC8800",
    "terminal.ansiCyan": "#FFAA00",
    "terminal.ansiWhite": "#FFB000",
    "terminal.ansiBrightBlack": "#997700",
    "terminal.ansiBrightRed": "#FFE000",
    "terminal.ansiBrightGreen": "#FFCC00",
    "terminal.ansiBrightYellow": "#FFE000",
    "terminal.ansiBrightBlue": "#FFB000",
    "terminal.ansiBrightMagenta": "#FFB000",
    "terminal.ansiBrightCyan": "#FFCC00",
    "terminal.ansiBrightWhite": "#FFE000",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#FFB000",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#FFB000",
    "button.background": "#000000",
    "button.hoverBackground": "#1A1A00",
    "dropdown.background": "#000000",
    "dropdown.foreground": "#FFB000",
    "input.background": "#000000",
    "input.foreground": "#FFB000",
    "selection.background": "rgba(255, 176, 0, 0.2)",
    "scrollbarSlider.background": "rgba(255, 176, 0, 0.1)",
    "scrollbarSlider.hoverBackground": "rgba(255, 176, 0, 0.5)"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#997700"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#FFAA00"
      }
    },
    {
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#CC8800"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#FFB000"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#FFAA00"
      }
    }
  ]
}