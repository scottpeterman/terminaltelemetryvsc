  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#55FF55"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#0080FF"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#FFFF55"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call.generic",
        "support.function"
      ],
      "settings": {
        "foreground": "#55FFFF"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#FF55FF"
      }
    }
  ]
}