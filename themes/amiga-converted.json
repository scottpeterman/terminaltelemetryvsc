{
  "name": "Imported Custom Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#AAAAAA",
    "editor.foreground": "#000000",
    "activityBar.background": "#FF0000",
    "sideBar.background": "#888888",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#000000",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#AA0000",
    "terminal.ansiGreen": "#00AA00",
    "terminal.ansiYellow": "#AA5500",
    "terminal.ansiBlue": "#0055AA",
    "terminal.ansiMagenta": "#AA00AA",
    "terminal.ansiCyan": "#00AAAA",
    "terminal.ansiWhite": "#AAAAAA",
    "terminal.ansiBrightBlack": "#555555",
    "terminal.ansiBrightRed": "#FF0000",
    "terminal.ansiBrightGreen": "#00FF00",
    "terminal.ansiBrightYellow": "#FFFF00",
    "terminal.ansiBrightBlue": "#0055FF",
    "terminal.ansiBrightMagenta": "#FF00FF",
    "terminal.ansiBrightCyan": "#00FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "statusBar.background": "#FF0000",
    "statusBar.foreground": "#000000",
    "titleBar.activeBackground": "#0055AA",
    "titleBar.activeForeground": "#000000",
    "button.background": "#FF0000",
    "button.hoverBackground": "#0055AA",
    "dropdown.background": "#FF0000",
    "dropdown.foreground": "#000000",
    "input.background": "#FF0000",
    "input.foreground": "#000000",
    "selection.background": "#0055AA",
    "scrollbarSlider.background": "#DDDDDD",
    "scrollbarSlider.hoverBackground": "#888888"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#00AA00"
      }
    },
    {
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#0055AA"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#AA5500"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#00AAAA"
      }
    }
  ]
}